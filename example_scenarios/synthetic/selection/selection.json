{
  "application_name": "selection",
  "optimization_objectives": [
    "runtime"
  ],
  "feasible_output": {
    "enable_feasible_predictor": true,
    "name": "Valid",
    "true_value": "True",
    "false_value": "False"
  },
  "output_data_file": "example_scenarios/synthetic/selection/csv/selection.csv",
  "log_file": "example_scenarios/synthetic/selection/selection.log",
  "epsilon_greedy_threshold": 0,
  "models": {
    "model": "gaussian_process"
  },
  "design_of_experiment": {
    "doe_type" : "random sampling",
    "number_of_samples": 100
  },
  "optimization_method": "bayesian_optimization",
  "optimization_iterations": 0,
  "input_parameters": {
    "rewriting": {
        "parameter_type": "selection",
        "values": ["A", "B", "C"],
        "length": [1, 5],
        "constraints": ["rewriting_NB<=0", "rewriting_0 = A", "rewriting_iC=3,4"],
        "dependencies": [],
        "parametrization" : "kendall"
    }
  }
}
